FoamFile
{
	format ascii;
	class volVectorField;
	object U;
}

Uinlet 20;
dimensions [0 1 -1 0 0 0 0];

internalField uniform ($Uinlet 0 0);

boundaryField
{
	inlet
	{
		type freestreamVelocity;
		freestreamValue uniform ($Uinlet 0 0);
	}

	freestream
	{
		type freestreamVelocity;
		freestreamValue uniform ($Uinlet 0 0);
	}

	body
	{
		type noSlip;
	}
	
	center
	{
		type symmetryPlane;
	}
}
