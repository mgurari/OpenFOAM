FoamFile
{
	format ascii;
	class dictionary;
	object fvSchemes;
}

ddtSchemes
{
	default Euler;
}

gradSchemes
{
	default Gauss linear;
	
	grad(U) cellLimited Gauss linear 1;
	grad(nuTilda)	Gauss linear;
}

divSchemes
{
	default none;
	turbulence Gauss limitedLinear 1;

	div(phi, U) Gauss linearUpwind limited; 	
	div((phi, nuTilda) Gauss linear;
}

snGradSchemes
{
	default corrected;
}

laplacianSchemes
{
	default Gauss linear corrected;
}

interpolationSchemes
{
	default linear;
}

wallDist
{
	method meshWave;
}
