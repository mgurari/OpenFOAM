FoamFile
{
	format ascii;
	class dictionary;
	object fvSchemes;
}

ddtSchemes
{
	default CrankNicolson 0.9;
}

gradSchemes
{
	default Gauss linear;
}

divSchemes
{
	default none;

	div(phi,U) Gauss limitedLinearV 1.0; //maybe limitedLinear 1.0 everywhere
	div(phi,k) Gauss linear;
	div(phi,omega) Gauss linear;
	div(phi,epsilon) Gauss linear; 
	div((nuEff*dev2(T(grad(U))))) Gauss linear;
}

snGradSchemes
{
	default corrected;
}

laplacianSchemes
{
	default Gauss linear corrected;
}

interpolationSchemes
{
	default linear;
}

wallDist
{
	method meshWave;
}
